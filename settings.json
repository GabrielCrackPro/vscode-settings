{
  // File-specific formatter settings
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  // Editor appearance and behavior
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "CaskaydiaCove Nerd Font",
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.wordWrap": "on",
  "editor.tabSize": 2,
  "editor.renderWhitespace": "none",
  "editor.cursorBlinking": "expand",
  "editor.lineNumbers": "relative",
  "editor.cursorStyle": "line-thin",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.guides.bracketPairs": true,
  "editor.guides.indentation": false,
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.semanticHighlighting.enabled": true,
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": false,
  "editor.wordBasedSuggestions": "matchingDocuments",
  "editor.minimap.enabled": false,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.horizontal": "hidden",
  "editor.overviewRulerBorder": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.definitionLinkOpensInPeek": true,
  "editor.lightbulb.enabled": "off",
  "editor.parameterHints.enabled": true,
  "editor.roundedSelection": true,
  "editor.suggest.preview": true,
  "editor.suggest.showStatusBar": true,
  // Formatting and code actions
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "off",
    "strings": "off"
  },
  "editor.suggestSelection": "recentlyUsed",
  "eslint.format.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  // Terminal settings
  "terminal.external.osxExec": "Warp.app",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontFamily": "CaskaydiaCove Nerd Font Mono",
  "terminal.integrated.minimumContrastRatio": 1,
  "terminal.integrated.shellIntegration.decorationsEnabled": "both",
  "terminal.integrated.shellIntegration.enabled": true,
  "terminal.integrated.suggest.enabled": true,
  "terminal.integrated.smoothScrolling": true,
  // Workbench and UI customizations
  "workbench.settings.editor": "json",
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.iconTheme": "catppuccin noctis icons",
  "workbench.editor.enablePreview": false,
  "workbench.editor.openPositioning": "last",
  "workbench.editor.tabSizing": "fit",
  "workbench.editor.languageDetection": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.decorations.badges": true,
  "workbench.editor.decorations.colors": true,
  "workbench.editor.tabActionCloseVisibility": false,
  "workbench.editor.empty.hint": "hidden",
  "workbench.sideBar.location": "right",
  "workbench.layoutControl.enabled": false,
  "workbench.list.smoothScrolling": true,
  "workbench.startupEditor": "none",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.commandPalette.experimental.suggestCommands": true,
  "workbench.commandPalette.history": 5,
  "editor.codeActionWidget.showHeaders": false,
  "editor.copyWithSyntaxHighlighting": false,
  // Explorer settings
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 1,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "package.json": "package.json, package-lock.json, pnpm-lock.yaml, eslint* ,commitlint.config.js, .stylelintrc.json, .prettierrc, .editorconfig, .browserslistrc, karma.conf.js, metro.config.js, .gitignore, tailwind.config.js,postcss.config.js,eslint.config.js,biome.json, vite*",
    ".env": ".env, .env.*",
    "tsconfig.json": "tsconfig*"
  },
  "explorer.fileNesting.expand": false,
  "explorer.compactFolders": false,
  "explorer.sortOrderLexicographicOptions": "lower",
  // Search and security settings
  "search.exclude": {
    "**/node_modules": true
  },
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.untrustedFiles": "open",
  // Git and version control
  "git.openRepositoryInParentFolders": "never",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  // Custom labels for different file types
  "workbench.editor.customLabels.enabled": true,
  "workbench.editor.customLabels.patterns": {
    // Angular files
    "**/*module*": "${filename} - Module üìö",
    "**/*component*": "${filename} - Component üß©",
    "**/*service*": "${filename} - Service üì°",
    "**/*pipe*": "${filename} - Pipe ü™à",
    "**/*resolver*": "${filename} - Resolver üîç",
    "**/*routes*": "${filename} - Routes üìç",
    // React files
    "**/hooks/**/*.{js,jsx,ts,tsx}": "${filename} - Hook üé£",
    "**/contexts/**/*.{js,jsx,ts,tsx}": "${filename} - Context üåê",
    "**/screens/**/*.{js,jsx,ts,tsx}": "${filename} - Screen üì±",
    "**/navigators/**/*.{js,jsx,ts,tsx}": "${filename} - Navigator üó∫Ô∏è",
    // Common files
    "**/*model*": "${filename} - Model üìò",
    "**/*enum*": "${filename} - Enum üìò",
    "**/package*": "${filename}.${extname} - Package üì¶",
    "**/*.scss": "${filename}.${extname} - Styles üé®",
    "**/*.css": "${filename}.${extname} - Styles üé®",
    "**/*.json": "${filename} - JSON üìú",
    "**/*.md": "${filename} - Markdown üìù",
    "**/utils/**/*.{js,jsx,ts,tsx}": "${filename} - Uitl üõ†Ô∏è",
    "**/config/**/*.{js,jsx,ts,tsx}": "${filename} - Config ‚öôÔ∏è",
    "**/*config.{js,jsx,ts,tsx}": "${filename} - Config ‚öôÔ∏è",
    "**/constants/**/*.{js,jsx,ts,tsx}": "${filename} - Constants üßÆ",
    "**/index.{js,jsx,ts,tsx}": "${filename} - Index üóÇÔ∏è"
  },
  // VIM extension settings
  "vim.leader": "<space>",
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.enableNeovim": true,
  "vim.showcmd": false,
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "<Esc>",
        "<Esc>"
      ],
      "commands": [
        "workbench.action.files.save"
      ],
      "silent": true
    },
    {
      "before": [
        "g",
        "l"
      ],
      "commands": [
        "workbench.action.gotoLine"
      ],
      "silent": true
    },
    {
      "before": [
        "g",
        "s"
      ],
      "commands": [
        "workbench.action.gotoSymbol"
      ],
      "silent": true
    },
    {
      "before": [
        ":"
      ],
      "commands": [
        "workbench.action.showCommands"
      ],
      "silent": true
    },
    {
      "before": "<leader>o",
      "commands": [
        "editor.action.organizeImports"
      ],
      "silent": true
    }
  ],
  // Miscellaneous settings
  "errorLens.gutterIconsEnabled": true,
  "errorLens.gutterIconSet": "defaultOutline",
  "errorLens.fontFamily": "CaskaydiaCove Nerd Font",
  "errorLens.messageTemplate": "[$source] $message",
  "errorLens.statusBarMessageTemplate": "[$source] $message",
  "errorLens.fontWeight": "bold",
  "errorLens.fontStyleItalic": true,
  "errorLens.scrollbarHackEnabled": true,
  "errorLens.statusBarMessageEnabled": true,
  "errorLens.editorHoverPartsEnabled": {
    "messageEnabled": true
  },
  "errorLens.errorGutterIconColor": "#f38ba8",
  "errorLens.statusBarColorsEnabled": true,
  "window.dialogStyle": "custom",
  "window.titleBarStyle": "custom",
  "material-icon-theme.hidesExplorerArrows": true,
  "extensions.ignoreRecommendations": true,
  "vsintellicode.modify.editor.suggestSelection": "disabled",
  "editor.inlineSuggest.showToolbar": "always",
  "editor.inlineSuggest.syntaxHighlightingEnabled": true,
  "workbench.activityBar.location": "bottom",
  "editor.accessibilitySupport": "off",
  "workbench.tree.renderIndentGuides": "none",
  "workbench.tips.enabled": false,
  "workbench.navigationControl.enabled": false,
  "chat.commandCenter.enabled": false,
  "spotify.showNextButton": false,
  "spotify.showPreviousButton": false,
  "spotify.showLyricsButton": false,
  "spotify.showPlayPauseButton": false,
  "spotify.showSignInButton": false,
  "spotify.showSignOutButton": false,
  "spotify.showMuteUnmuteVolumeButton": false,
  "catppuccin-noctis-icons.hidesExplorerArrows": true,
  "catppuccin.accentColor": "mauve",
  "catppuccin.boldKeywords": true,
  "catppuccin.italicComments": true,
  "catppuccin.colorOverrides": {
    "mocha": {
      "mauve": "#9DDEEF"
    }
  },
  "catppuccin.customUIColors": {
    "mocha": {
      "breadcrumb.foreground": "overlay0",
      "breadcrumb.background": "crust",
      "breadcrumbPicker.background": "crust",
      "statusBar.background": "crust",
      "statusBar.border": "crust",
      "statusBar.foreground": "accent",
      "statusBar.noFolderBackground": "crust",
      "statusBar.noFolderForeground": "accent",
      "statusBar.noFolderBorder": "crust",
      "editorSuggestWidget.foreground": "overlay1",
      "editorSuggestWidget.selectedForeground": "accent",
      "editorSuggestWidget.border": "crust",
      "editorSuggestWidget.background": "crust",
      "commandCenter.foreground": "accent",
      "panel.background": "crust",
      "panelTitle.activeForeground": "accent",
      "panel.border": "crust",
      "input.background": "crust",
