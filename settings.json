      "notifications.border": "crust",
      "tab.activeBackground": "crust",
      "tab.inactiveBackground": "crust",
      "tab.activeModifiedBorder": "red",
      "tab.inactiveModifiedBorder": "red",
      "statusBarItem.errorBackground": "red",
      "keybindingLabel.foreground": "accent",
      "keybindingLabel.border": "crust",
      "keybindingLabel.background": "crust",
      "progressBar.background": "accent",
      "sideBar.background": "crust",
      "sideBarSectionHeader.background": "crust",
      "sideBarSectionHeader.foreground": "accent",
      "sideBar.foreground": "overlay0",
      "editorOverviewRuler.background": "base",
      "editorOverviewRuler.findMatchForeground": "accent",
      "editorOverviewRuler.warningForeground": "yellow",
      "editorOverviewRuler.errorForeground": "red",
      "editorWidget.background": "crust",
