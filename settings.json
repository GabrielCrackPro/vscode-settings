      "editor.findMatchBackground": "yellow",
      "editorHoverWidget.border": "crust",
      "editorHoverWidget.background": "crust",
      "editorHoverWidget.foreground": "overlay1",
      "list.activeSelectionBackground": "base",
      "list.inactiveSelectionBackground": "base",
      "list.dropBackground": "accent",
      "list.errorForeground": "red",
      "list.warningForeground": "yellow",
      "list.focusBackground": "base",
      "list.focusForeground": "accent",
      "list.highlightForeground": "accent",
      "list.inactiveFocusBackground": "base",
      "list.inactiveSelectionForeground": "accent",
      "list.inactiveSelectionIconForeground": "accent",
      "scrollbarSlider.background": "overlay2",
      "scrollbar.shadow": "crust",
      "button.secondaryBackground": "base",
      "notificationCenter.border": "crust",
      "notificationCenterHeader.background": "crust",
      "notificationCenterHeader.foreground": "accent",
      "notificationToast.border": "crust",
      "peekView.border": "crust",
      "peekViewEditorGutter.background": "crust",
      "peekViewResult.selectionForeground": "overlay0",
      "peekViewTitle.background": "crust",
      "peekViewTitleLabel.foreground": "accent",
      "peekViewTitleDescription.foreground": "overlay0"
    }
  },
  "editor.tokenColorCustomizations": {
    "[Catppuccin Mocha]": {
      "comments": "#585b70",
      "types": "#cba6f7",
      "textMateRules": [
        {
          "scope": "comment",
          "settings": {
            "fontStyle": "italic bold"
          }
        }
      ]
    }
  },
  "doki.sticker.css": "z-index:999;background-position:100% 92%;background-size:120px auto;",
}